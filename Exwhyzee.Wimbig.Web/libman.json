{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@aspnet/signalr@1.1.0",
      "destination": "wwwroot/lib/@aspnet/signalr/"
    },
    {
      "provider": "cdnjs",
      "library": "signalr.js@2.4.0",
      "destination": "wwwroot/lib/aspnet/signalr.js/",
      "files": [
        "jquery.signalR.min.js",
        "jquery.signalR.js"
      ]
    }
  ]
}